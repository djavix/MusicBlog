CREATE TABLE USUARIO
(
	ID int IDENTITY(1,1),
	NICK varchar(50) not null,
	PASSWORD varchar(50) not null	
)

CREATE TABLE PERFIL
(
	ID int IDENTITY(1,1),
	NOMBRE varchar(50) not null,
	APELLIDO varchar(50) not null,
	FECHANACIMIENTO date not null,
	SEXO char(1) not null,
	CORREO varchar(50) not null,
	FK_USUARIO int
)

CREATE TABLE ARTISTA
(
	ID int IDENTITY(1,1),
	NOMBRE varchar(50) not null
)

CREATE TABLE GRUPO
(
	ID int IDENTITY(1,1),
	NOMBRE varchar(50) not null
)

CREATE TABLE PERTENECE
(
	FK_ARTISTA int,
	FK_GRUPO int,
	FECHAINGRESO date not null,
	FECHASALIDA date
)

CREATE TABLE BIOGRAFIA
(
	ID int IDENTITY(1,1),
	NOMBRE varchar(50) not null,
	APELLIDO varchar(50),
	FECHANACIMIENTO date not null,
	HISTORIA varchar(max) not null,
	FK_ARTISTA int,
	FK_GRUPO int
)

CREATE TABLE GENERO
(
	ID int IDENTITY(1,1),
	NOMBRE varchar(50) not null
)

CREATE TABLE DISCO
(
	ID int IDENTITY(1,1),
	NOMBRE varchar(50) not null,
	FK_ARTISTA int,
	FK_GRUPO
)

CREATE TABLE CANCION
(
	ID int IDENTITY(1,1),
	NOMBRE varchar(50) not null,
	DURACION time not null,
	BPM int,
	FK_DISCO int,
	FK_GENERO int
)

CREATE TABLE LISTADEREPRODUCION
(
	ID int IDENTITY(1,1),
	NOMBRE varchar(50) not null,
	FECHA date not null,
	DESCRIPCION varchar(max) not null,
	FK_USUARIO int
)

CREATE TABLE MENU
(
	ID int IDENTITY(1,1),
	NOMBREPAGINA varchar(max) not null,
	URL varchar(max) not null
)

CREATE TABLE CANCION_LISTA
(
	FK_CANCION int,
	FK_LISTA int
)

CREATE TABLE MENU_USUARIO
(
	FK_USUARIO int,
	FK_MENU int
)




